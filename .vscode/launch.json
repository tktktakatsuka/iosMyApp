{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Expo App",
        "cwd": "${workspaceFolder}",
        "type": "reactnative",
        "request": "launch",
        "platform": "ios", // or "android"
        "target": "expo",
        "enableDebug": true
      },    
      {
        "name": "Attach to packager",
        "cwd": "${workspaceFolder}",
        "type": "reactnative",
        "request": "attach",
        // Expoのデバッグポートにアタッチ
        "port": 19001
      }
    ]
  }
  