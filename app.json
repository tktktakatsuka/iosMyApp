{
  "expo": {
    "name": "収支管理アプリ コツコツ貯めよう",
    "slug": "MyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/profitDataIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.s.takatsuka.MyApp",
      "infoPlist": {
        "GADApplicationIdentifier": "ca-app-pub-4855274440005459~3152347682",
        "NSUserTrackingUsageDescription": "このアプリは広告の効果を向上させるためにトラッキングを利用します。",
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "写真を撮影して収支記録に添付するためにカメラを使用します。",
        "CFBundleDisplayName": "収支管理アプリ コツコツ貯めよう"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3499a58e-a1af-440a-87e1-5a71c8ea4284"
      }
    }
  }
}